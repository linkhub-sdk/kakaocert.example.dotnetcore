@{
	ViewBag.Title = "Kakaocert SDK ASP.NET Core Example.";
}

<div id="content">
	<p class="heading1">@ViewData["Title"]</p>
	<br />
	<fieldset class="fieldset1">
		<legend>전자서명 API</legend>
		<ul>
			<li><a href="Kakaocert/RequestESign">RequestESign</a> (전자서명 요청)</li>
			<li><a href="Kakaocert/GetESignState">GetESignState</a> (전자서명 서명상태 확인)</li>
			<li><a href="Kakaocert/VerifyESign">VerifyESign</a> (전자서명 서명검증)</li>
		</ul>
	</fieldset>
	<br />
	<fieldset class="fieldset1">
		<legend>본인인증 API</legend>
		<ul>
			<li><a href="Kakaocert/RequestVerifyAuth">RequestVerifyAuth</a> (본인인증 요청)</li>
			<li><a href="Kakaocert/GetVerifyAuthState">GetVerifyAuthState</a> (본인인증 서명상태 확인)</li>
			<li><a href="Kakaocert/VerifyAuth">VerifyAuth</a> (본인인증 서명검증)</li>
		</ul>
	</fieldset>
	<br />
	<fieldset class="fieldset1">
		<legend>자동이체 출금동의 API</legend>
		<ul>
			<li><a href="Kakaocert/RequestCMS">RequestCMS</a> (자동이체 출금동의 요청)</li>
			<li><a href="Kakaocert/GetCMSState">GetCMSState</a> (자동이체 출금동의 서명상태 확인)</li>
			<li><a href="Kakaocert/VerifyCMS">VerifyCMS</a> (자동이체 출금동의 서명검증)</li>
		</ul>
	</fieldset>

</div>
